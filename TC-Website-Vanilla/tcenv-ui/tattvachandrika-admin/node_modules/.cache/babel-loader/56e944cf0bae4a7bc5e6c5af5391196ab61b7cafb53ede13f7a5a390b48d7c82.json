{"ast":null,"code":"import magazineSubscriberService from '@/services/magazineSubscriberService';\nimport AddSubscriptionModal from './AddSubscriptionModal.vue';\nexport default {\n  components: {\n    AddSubscriptionModal\n  },\n  data() {\n    return {\n      subscriber: {},\n      activeTab: 'active',\n      activeSubscriptions: [],\n      inactiveSubscriptions: []\n    };\n  },\n  methods: {\n    fetchSubscriberDetails() {\n      const id = this.$route.params.id;\n      magazineSubscriberService.getSubscriber(id).then(response => {\n        this.subscriber = response.data;\n        this.activeSubscriptions = response.data.subscriptions.filter(sub => sub.active);\n        this.inactiveSubscriptions = response.data.subscriptions.filter(sub => !sub.active);\n      });\n    },\n    openAddSubscriptionModal() {\n      this.$refs.addSubscriptionModal.open();\n    },\n    closeAddSubscriptionModal() {\n      this.fetchSubscriberDetails();\n    }\n  },\n  mounted() {\n    this.fetchSubscriberDetails();\n  }\n};","map":{"version":3,"names":["magazineSubscriberService","AddSubscriptionModal","components","data","subscriber","activeTab","activeSubscriptions","inactiveSubscriptions","methods","fetchSubscriberDetails","id","$route","params","getSubscriber","then","response","subscriptions","filter","sub","active","openAddSubscriptionModal","$refs","addSubscriptionModal","open","closeAddSubscriptionModal","mounted"],"sources":["C:\\Users\\santoshdesai\\Tattvachandrika\\tcenv-ui\\tattvachandrika-admin\\src\\components\\AddEditSubscriberModal.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <h1>Subscriber Details</h1>\r\n    <button @click=\"$router.push({ name: 'MagazineSubscribers' })\">Back to Subscribers</button>\r\n    <div>\r\n      <h2>{{ subscriber.name }}</h2>\r\n      <p><strong>Registration Number:</strong> {{ subscriber.registration_number }}</p>\r\n      <p><strong>Address:</strong> {{ subscriber.address }}</p>\r\n      <p><strong>City/Town:</strong> {{ subscriber.city_town }}</p>\r\n      <p><strong>State:</strong> {{ subscriber.state }}</p>\r\n      <p><strong>Pincode:</strong> {{ subscriber.pincode }}</p>\r\n      <p><strong>Phone:</strong> {{ subscriber.phone }}</p>\r\n      <p><strong>Email:</strong> {{ subscriber.email }}</p>\r\n      <p><strong>Category:</strong> {{ subscriber.category.name }}</p>\r\n      <p><strong>Type:</strong> {{ subscriber.stype.name }}</p>\r\n      <p><strong>Notes:</strong> {{ subscriber.notes }}</p>\r\n      <button @click=\"openAddSubscriptionModal\">Add Subscription</button>\r\n    </div>\r\n    <div>\r\n      <ul>\r\n        <li @click=\"activeTab = 'active'\" :class=\"{ active: activeTab === 'active' }\">Active Subscriptions</li>\r\n        <li @click=\"activeTab = 'inactive'\" :class=\"{ active: activeTab === 'inactive' }\">Inactive Subscriptions</li>\r\n      </ul>\r\n      <div v-if=\"activeTab === 'active'\">\r\n        <h3>Active Subscriptions</h3>\r\n        <table>\r\n          <thead>\r\n            <tr>\r\n              <th>Plan Name</th>\r\n              <th>Start Date</th>\r\n              <th>End Date</th>\r\n            </tr>\r\n          </thead>\r\n          <tbody>\r\n            <tr v-for=\"subscription in activeSubscriptions\" :key=\"subscription._id\">\r\n              <td>{{ subscription.subscription_plan.name }}</td>\r\n              <td>{{ subscription.start_date }}</td>\r\n              <td>{{ subscription.end_date }}</td>\r\n            </tr>\r\n          </tbody>\r\n        </table>\r\n      </div>\r\n      <div v-if=\"activeTab === 'inactive'\">\r\n        <h3>Inactive Subscriptions</h3>\r\n        <table>\r\n          <thead>\r\n            <tr>\r\n              <th>Plan Name</th>\r\n              <th>Start Date</th>\r\n              <th>End Date</th>\r\n            </tr>\r\n          </thead>\r\n          <tbody>\r\n            <tr v-for=\"subscription in inactiveSubscriptions\" :key=\"subscription._id\">\r\n              <td>{{ subscription.subscription_plan.name }}</td>\r\n              <td>{{ subscription.start_date }}</td>\r\n              <td>{{ subscription.end_date }}</td>\r\n            </tr>\r\n          </tbody>\r\n        </table>\r\n      </div>\r\n    </div>\r\n    <add-subscription-modal\r\n      :subscriber-id=\"subscriber._id\"\r\n      @save=\"fetchSubscriberDetails\"\r\n      @close=\"closeAddSubscriptionModal\"\r\n    />\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport magazineSubscriberService from '@/services/magazineSubscriberService';\r\nimport AddSubscriptionModal from './AddSubscriptionModal.vue';\r\n\r\nexport default {\r\n  components: {\r\n    AddSubscriptionModal,\r\n  },\r\n  data() {\r\n    return {\r\n      subscriber: {},\r\n      activeTab: 'active',\r\n      activeSubscriptions: [],\r\n      inactiveSubscriptions: [],\r\n    };\r\n  },\r\n  methods: {\r\n    fetchSubscriberDetails() {\r\n      const id = this.$route.params.id;\r\n      magazineSubscriberService.getSubscriber(id).then((response) => {\r\n        this.subscriber = response.data;\r\n        this.activeSubscriptions = response.data.subscriptions.filter(sub => sub.active);\r\n        this.inactiveSubscriptions = response.data.subscriptions.filter(sub => !sub.active);\r\n      });\r\n    },\r\n    openAddSubscriptionModal() {\r\n      this.$refs.addSubscriptionModal.open();\r\n    },\r\n    closeAddSubscriptionModal() {\r\n      this.fetchSubscriberDetails();\r\n    },\r\n  },\r\n  mounted() {\r\n    this.fetchSubscriberDetails();\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n/* Add your styles here */\r\n.active {\r\n  font-weight: bold;\r\n}\r\n</style>\r\n"],"mappings":"AAuEA,OAAOA,yBAAwB,MAAO,sCAAsC;AAC5E,OAAOC,oBAAmB,MAAO,4BAA4B;AAE7D,eAAe;EACbC,UAAU,EAAE;IACVD;EACF,CAAC;EACDE,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,UAAU,EAAE,CAAC,CAAC;MACdC,SAAS,EAAE,QAAQ;MACnBC,mBAAmB,EAAE,EAAE;MACvBC,qBAAqB,EAAE;IACzB,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACPC,sBAAsBA,CAAA,EAAG;MACvB,MAAMC,EAAC,GAAI,IAAI,CAACC,MAAM,CAACC,MAAM,CAACF,EAAE;MAChCV