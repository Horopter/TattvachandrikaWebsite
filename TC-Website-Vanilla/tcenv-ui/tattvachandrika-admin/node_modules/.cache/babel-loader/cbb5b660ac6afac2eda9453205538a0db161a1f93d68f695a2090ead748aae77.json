ion)\r\n          .then(() => {\r\n            this.loadSubscriberDetails();\r\n            this.showAddEditSubscriptionModal = false;\r\n          })\r\n          .catch(error => {\r\n            console.error(\"There was an error updating the subscription!\", error);\r\n          });\r\n      } else {\r\n        subscription.subscriber = this.subscriber._id;\r\n        axios.post('http://localhost:8000/api/subscriptions/', subscription)\r\n          .then(() => {\r\n            this.loadSubscriberDetails();\r\n            this.showAddEditSubscriptionModal = false;\r\n          })\r\n          .catch(error => {\r\n            console.error(\"There was an error adding the subscription!\", error);\r\n          });\r\n      }\r\n    },\r\n    editSubscription(subscription) {\r\n      this.selectedSubscription = { ...subscription };\r\n      this.showAddEditSubscriptionModal = true;\r\n    },\r\n    confirmPaymentSuccess(subscription) {\r\n      if (subscription.payment_status === 'Successful') {\r\n        this.subscriptionToConfirm = subscription;\r\n        this.showConfirmationModal = true;\r\n      }\r\n    },\r\n    hideConfirmationModal() {\r\n      this.showConfirmationModal = false;\r\n    },\r\n    markPaymentSuccess() {\r\n      if (this.subscriptionToConfirm) {\r\n        axios.put(`http://localhost:8000/api/subscriptions/${this.subscriptionToConfirm._id}/`, {\r\n          ...this.subscriptionToConfirm,\r\n          payment_status: 'Successful'\r\n        })\r\n          .then(() => {\r\n            this.loadSubscriberDetails();\r\n            this.hideConfirmationModal();\r\n          })\r\n          .catch(error => {\r\n            console.error(\"There was an error marking the payment as successful!\", error);\r\n          });\r\n      }\r\n    },\r\n    getPlanName(planId) {\r\n      const plan = this.plans.find(p => p._id === planId);\r\n      return plan ? plan.name : '';\r\n    },\r\n    getPaymentModeName(paymentModeId) {\r\n      const paymentMode 