{"ast":null,"code":"import addEditSubscriberModal from './AddEditSubscriberModal.vue';\nimport confirmationModal from './ConfirmationModal.vue';\nimport magazineSubscriberService from '../services/magazineSubscriberService';\nexport default {\n  components: {\n    addEditSubscriberModal,\n    confirmationModal\n  },\n  data() {\n    return {\n      subscribers: [],\n      categories: [],\n      types: [],\n      showAddEditModal: false,\n      showConfirmationModal: false,\n      selectedSubscriber: null,\n      subscriberToDelete: null\n    };\n  },\n  computed: {\n    currentSubscribers() {\n      return this.subscribers.filter(subscriber => !subscriber.isDeleted && subscriber.hasActiveSubscriptions);\n    },\n    waitingSubscribers() {\n      return this.subscribers.filter(subscriber => !subscriber.isDeleted && !subscriber.hasActiveSubscriptions);\n    },\n    inactiveSubscribers() {\n      return this.subscribers.filter(subscriber => subscriber.isDeleted);\n    }\n  },\n  created() {\n    this.loadSubscribers();\n    this.loadCategories();\n    this.loadTypes();\n  },\n  methods: {\n    loadSubscribers() {\n      magazineSubscriberService.getMagazineSubscribers().then(response => {\n        this.subscribers = response.data;\n      }).catch(error => {\n        console.error(\"There was an error retrieving the subscribers!\", error);\n      });\n    },\n    loadCategories() {\n      magazineSubscriberService.getCategories().then(response => {\n        this.categories = response.data;\n      }).catch(error => {\n        console.error(\"There was an error retrieving the categories!\", error);\n      });\n    },\n    loadTypes() {\n      magazineSubscriberService.getTypes().then(response => {\n        this.types = response.data;\n      }).catch(error => {\n        console.error(\"There was an error retrieving the types!\", error);\n      });\n    },\n    addSubscriber() {\n      this.selectedSubscriber = {};\n      this.showAddEditModal = true;\n    },\n    editSubscriber(subscriber) {\n      this.selectedSubscriber = {\n        ...subscriber\n      };\n      this.showAddEditModal = true;\n    },\n    saveSubscriber(subscriber) {\n      if (subscriber._id) {\n        magazineSubscriberService.updateMagazineSubscriber(subscriber._id, subscriber).then(() => {\n          this.loadSubscribers();\n        }).catch(error => {\n          console.error(\"There was an error updating the subscriber!\", error);\n        });\n      } else {\n        magazineSubscriberService.createMagazineSubscriber(subscriber).then(() => {\n          this.loadSubscribers();\n        }).catch(error => {\n          console.error(\"There was an error creating the subscriber!\", error);\n        });\n      }\n      this.showAddEditModal = false;\n    },\n    confirmDeleteSubscriber(subscriberId) {\n      this.subscriberToDelete = subscriberId;\n      this.showConfirmationModal = true;\n    },\n    hideDeleteModal() {\n      this.showConfirmationModal = false;\n    },\n    deleteSubscriber() {\n      if (this.subscriberToDelete) {\n        magazineSubscriberService.deleteMagazineSubscriber(this.subscriberToDelete).then(() => {\n          this.loadSubscribers();\n          this.hideDeleteModal();\n        }).catch(error => {\n          console.error(\"There was an error deleting the subscriber!\", error);\n        });\n      }\n    },\n    activateSubscriber(subscriberId) {\n      magazineSubscriberService.activateMagazineSubscriber(subscriberId).then(() => {\n        this.loadSubscribers();\n      }).catch(error => {\n        console.error(\"There was an error activating the subscriber!\", error);\n      });\n    },\n    getCategoryName(categoryId) {\n      const category = this.categories.find(cat => cat._id === categoryId);\n      return category ? category.name : '';\n    },\n    getTypeName(typeId) {\n      const type = this.types.find(t => t._id === typeId);\n      return type ? type.name : '';\n    },\n    viewSubscriber(subscriber) {\n      console.log(subscriber);\n      // Implement the logic to view subscriber details\n    }\n  }\n};","map":{"version":3,"names":["addEditSubscriberModal","confirmationModal","magazineSubscriberService","components","data","subscribers","categories","types","showAddEditModal","showConfirmationModal","selectedSubscriber","subscriberToDelete","computed","currentSubscribers","filter","subscriber","isDeleted","hasActiveSubscriptions","waitingSubscribers","inactiveSubscribers","created","loadSubscribers","loadCategories","loadTypes","methods","getMagazineSubscribers","then","response","catch","error","console","getCategories","getTypes","addSubscriber","editSubscriber","saveSubscriber","_id","updateMagazineSubscriber","createMagazineSubscriber","confirmDeleteSubscriber","subscriberId","hideDeleteModal","deleteSubscriber","deleteMagazineSubscriber","activateSubscriber","activateMagazineSubscriber","getCategoryName","categoryId","category","find","cat","name","getTypeName","typeId","type","t","viewSubscriber","log"],"sources":["C:\\Users\\santoshdesai\\Tattvachandrika\\tcenv-ui\\tattvachandrika-admin\\src\\components\\MagazineSubscribers.vue"],"sourcesContent":["<template>\r\n  <div class=\"container mt-5\">\r\n    <nav aria-label=\"breadcrumb\">\r\n      <ol class=\"breadcrumb\">\r\n        <li class=\"breadcrumb-item\"><router-link to=\"/\">Home</router-link></li>\r\n        <li class=\"breadcrumb-item active\" aria-current=\"page\">Magazine Subscribers</li>\r\n      </ol>\r\n    </nav>\r\n    <h2>Magazine Subscribers</h2>\r\n\r\n    <ul class=\"nav nav-tabs\" id=\"subscriberTabs\" role=\"tablist\">\r\n      <li class=\"nav-item\">\r\n        <a class=\"nav-link active\" id=\"active-tab\" data-toggle=\"tab\" href=\"#active\" role=\"tab\" aria-controls=\"active\" aria-selected=\"true\">Active Subscribers</a>\r\n      </li>\r\n      <li class=\"nav-item\">\r\n        <a class=\"nav-link\" id=\"inactive-tab\" data-toggle=\"tab\" href=\"#inactive\" role=\"tab\" aria-controls=\"inactive\" aria-selected=\"false\">Inactive Subscribers</a>\r\n      </li>\r\n    </ul>\r\n\r\n    <div class=\"tab-content\" id=\"subscriberTabsContent\">\r\n      <div class=\"tab-pane fade show active\" id=\"active\" role=\"tabpanel\" aria-labelledby=\"active-tab\">\r\n        <div class=\"tab-content\">\r\n          <div class=\"tab-pane fade show active\" id=\"current\" role=\"tabpanel\">\r\n            <h3>Current Subscribers</h3>\r\n            <button class=\"btn btn-primary mb-3\" @click=\"addSubscriber\">Add Subscriber</button>\r\n            <table class=\"table table-striped\">\r\n              <thead>\r\n                <tr>\r\n                  <th>Name</th>\r\n                  <th>Registration Number</th>\r\n                  <th>City/Town</th>\r\n                  <th>State</th>\r\n                  <th>Phone</th>\r\n                  <th>Email</th>\r\n                  <th>Category</th>\r\n                  <th>Type</th>\r\n                  <th>Actions</th>\r\n                </tr>\r\n              </thead>\r\n              <tbody>\r\n                <tr v-for=\"subscriber in currentSubscribers\" :key=\"subscriber._id\">\r\n                  <td>{{ subscriber.name }}</td>\r\n                  <td>{{ subscriber.registration_number }}</td>\r\n                  <td>{{ subscriber.city_town }}</td>\r\n                  <td>{{ subscriber.state }}</td>\r\n                  <td>{{ subscriber.phone }}</td>\r\n                  <td>{{ subscriber.email }}</td>\r\n                  <td>{{ getCategoryName(subscriber.category) }}</td>\r\n                  <td>{{ getTypeName(subscriber.stype) }}</td>\r\n                  <td>\r\n                    <button class=\"btn btn-warning btn-sm\" @click=\"editSubscriber(subscriber)\">Edit</button>\r\n                    <button class=\"btn btn-danger btn-sm\" @click=\"confirmDeleteSubscriber(subscriber._id)\">Delete</button>\r\n                    <button class=\"btn btn-info btn-sm\" @click=\"viewSubscriber(subscriber)\">View</button>\r\n                  </td>\r\n                </tr>\r\n              </tbody>\r\n            </table>\r\n          </div>\r\n          <div class=\"tab-pane fade\" id=\"waiting\" role=\"tabpanel\">\r\n            <h3>Waiting for Renewal</h3>\r\n            <!-- Similar table structure for Waiting for Renewal Subscribers -->\r\n          </div>\r\n        </div>\r\n      </div>\r\n      <div class=\"tab-pane fade\" id=\"inactive\" role=\"tabpanel\" aria-labelledby=\"inactive-tab\">\r\n        <h3>Inactive Subscribers</h3>\r\n        <table class=\"table table-striped\">\r\n          <thead>\r\n            <tr>\r\n              <th>Name</th>\r\n              <th>Registration Number</th>\r\n              <th>City/Town</th>\r\n              <th>State</th>\r\n              <th>Phone</th>\r\n              <th>Email</th>\r\n              <th>Category</th>\r\n              <th>Type</th>\r\n              <th>Actions</th>\r\n            </tr>\r\n          </thead>\r\n          <tbody>\r\n            <tr v-for=\"subscriber in inactiveSubscribers\" :key=\"subscriber._id\">\r\n              <td>{{ subscriber.name }}</td>\r\n              <td>{{ subscriber.registration_number }}</td>\r\n              <td>{{ subscriber.city_town }}</td>\r\n              <td>{{ subscriber.state }}</td>\r\n              <td>{{ subscriber.phone }}</td>\r\n              <td>{{ subscriber.email }}</td>\r\n              <td>{{ getCategoryName(subscriber.category) }}</td>\r\n              <td>{{ getTypeName(subscriber.stype) }}</td>\r\n              <td>\r\n                <button class=\"btn btn-success btn-sm\" @click=\"activateSubscriber(subscriber._id)\">Activate</button>\r\n              </td>\r\n            </tr>\r\n          </tbody>\r\n        </table>\r\n      </div>\r\n    </div>\r\n\r\n    <add-edit-subscriber-modal\r\n      :show=\"showAddEditModal\"\r\n      :subscriber=\"selectedSubscriber\"\r\n      :categories=\"categories\"\r\n      :types=\"types\"\r\n      @close=\"closeAddEditModal\"\r\n      @save=\"saveSubscriber\"\r\n    />\r\n\r\n    <confirmation-modal\r\n      v-if=\"showConfirmationModal\"\r\n      :show=\"showConfirmationModal\"\r\n      title=\"Confirm Delete\"\r\n      message=\"Are you sure you want to delete this subscriber?\"\r\n      @close=\"hideDeleteModal\"\r\n      @confirm=\"deleteSubscriber\"\r\n    />\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport addEditSubscriberModal from './AddEditSubscriberModal.vue';\r\nimport confirmationModal from './ConfirmationModal.vue';\r\nimport magazineSubscriberService from '../services/magazineSubscriberService';\r\n\r\nexport default {\r\n  components: {\r\n    addEditSubscriberModal,\r\n    confirmationModal\r\n  },\r\n  data() {\r\n    return {\r\n      subscribers: [],\r\n      categories: [],\r\n      types: [],\r\n      showAddEditModal: false,\r\n      showConfirmationModal: false,\r\n      selectedSubscriber: null,\r\n      subscriberToDelete: null,\r\n    };\r\n  },\r\n  computed: {\r\n    currentSubscribers() {\r\n      return this.subscribers.filter(subscriber => !subscriber.isDeleted && subscriber.hasActiveSubscriptions);\r\n    },\r\n    waitingSubscribers() {\r\n      return this.subscribers.filter(subscriber => !subscriber.isDeleted && !subscriber.hasActiveSubscriptions);\r\n    },\r\n    inactiveSubscribers() {\r\n      return this.subscribers.filter(subscriber => subscriber.isDeleted);\r\n    }\r\n  },\r\n  created() {\r\n    this.loadSubscribers();\r\n    this.loadCategories();\r\n    this.loadTypes();\r\n  },\r\n  methods: {\r\n    loadSubscribers() {\r\n      magazineSubscriberService.getMagazineSubscribers().then(response => {\r\n        this.subscribers = response.data;\r\n      }).catch(error => {\r\n        console.error(\"There was an error retrieving the subscribers!\", error);\r\n      });\r\n    },\r\n    loadCategories() {\r\n      magazineSubscriberService.getCategories().then(response => {\r\n        this.categories = response.data;\r\n      }).catch(error => {\r\n        console.error(\"There was an error retrieving the categories!\", error);\r\n      });\r\n    },\r\n    loadTypes() {\r\n      magazineSubscriberService.getTypes().then(response => {\r\n        this.types = response.data;\r\n      }).catch(error => {\r\n        console.error(\"There was an error retrieving the types!\", error);\r\n      });\r\n    },\r\n    addSubscriber() {\r\n      this.selectedSubscriber = {};\r\n      this.showAddEditModal = true;\r\n    },\r\n    editSubscriber(subscriber) {\r\n      this.selectedSubscriber = { ...subscriber };\r\n      this.showAddEditModal = true;\r\n    },\r\n    saveSubscriber(subscriber) {\r\n      if (subscriber._id) {\r\n        magazineSubscriberService.updateMagazineSubscriber(subscriber._id, subscriber).then(() => {\r\n          this.loadSubscribers();\r\n        }).catch(error => {\r\n          console.error(\"There was an error updating the subscriber!\", error);\r\n        });\r\n      } else {\r\n        magazineSubscriberService.createMagazineSubscriber(subscriber).then(() => {\r\n          this.loadSubscribers();\r\n        }).catch(error => {\r\n          console.error(\"There was an error creating the subscriber!\", error);\r\n        });\r\n      }\r\n      this.showAddEditModal = false;\r\n    },\r\n    confirmDeleteSubscriber(subscriberId) {\r\n      this.subscriberToDelete = subscriberId;\r\n      this.showConfirmationModal = true;\r\n    },\r\n    hideDeleteModal() {\r\n      this.showConfirmationModal = false;\r\n    },\r\n    deleteSubscriber() {\r\n      if (this.subscriberToDelete) {\r\n        magazineSubscriberService.deleteMagazineSubscriber(this.subscriberToDelete).then(() => {\r\n          this.loadSubscribers();\r\n          this.hideDeleteModal();\r\n        }).catch(error => {\r\n          console.error(\"There was an error deleting the subscriber!\", error);\r\n        });\r\n      }\r\n    },\r\n    activateSubscriber(subscriberId) {\r\n      magazineSubscriberService.activateMagazineSubscriber(subscriberId).then(() => {\r\n        this.loadSubscribers();\r\n      }).catch(error => {\r\n        console.error(\"There was an error activating the subscriber!\", error);\r\n      });\r\n    },\r\n    getCategoryName(categoryId) {\r\n      const category = this.categories.find(cat => cat._id === categoryId);\r\n      return category ? category.name : '';\r\n    },\r\n    getTypeName(typeId) {\r\n      const type = this.types.find(t => t._id === typeId);\r\n      return type ? type.name : '';\r\n    },\r\n    viewSubscriber(subscriber) {\r\n      console.log(subscriber);\r\n      // Implement the logic to view subscriber details\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.table {\r\n  margin-top: 20px;\r\n}\r\n</style>\r\n"],"mappings":"AAwHA,OAAOA,sBAAqB,MAAO,8BAA8B;AACjE,OAAOC,iBAAgB,MAAO,yBAAyB;AACvD,OAAOC,yBAAwB,MAAO,uCAAuC;AAE7E,eAAe;EACbC,UAAU,EAAE;IACVH,sBAAsB;IACtBC;EACF,CAAC;EACDG,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,WAAW,EAAE,EAAE;MACfC,UAAU,EAAE,EAAE;MACdC,KAAK,EAAE,EAAE;MACTC,gBAAgB,EAAE,KAAK;MACvBC,qBAAqB,EAAE,KAAK;MAC5BC,kBAAkB,EAAE,IAAI;MACxBC,kBAAkB,EAAE;IACtB,CAAC;EACH,CAAC;EACDC,QAAQ,EAAE;IACRC,kBAAkBA,CAAA,EAAG;MACnB,OAAO,IAAI,CAACR,WAAW,CAACS,MAAM,CAACC,UAAS,IAAK,CAACA,UAAU,CAACC,SAAQ,IAAKD,UAAU,CAACE,sBAAsB,CAAC;IAC1G,CAAC;IACDC,kBAAkBA,CAAA,EAAG;MACnB,OAAO,IAAI,CAACb,WAAW,CAACS,MAAM,CAACC,UAAS,IAAK,CAACA,UAAU,CAACC,SAAQ,IAAK,CAACD,UAAU,CAACE,sBAAsB,CAAC;IAC3G,CAAC;IACDE,mBAAmBA,CAAA,EAAG;MACpB,OAAO,IAAI,CAACd,WAAW,CAACS,MAAM,CAACC,UAAS,IAAKA,UAAU,CAACC,SAAS,CAAC;IACpE;EACF,CAAC;EACDI,OAAOA,CAAA,EAAG;IACR,IAAI,CAACC,eAAe,CAAC,CAAC;IACtB,IAAI,CAACC,cAAc,CAAC,CAAC;IACrB,IAAI,CAACC,SAAS,CAAC,CAAC;EAClB,CAAC;EACDC,OAAO,EAAE;IACPH,eAAeA,CAAA,EAAG;MAChBnB,yBAAyB,CAACuB,sBAAsB,CAAC,CAAC,CAACC,IAAI,CAACC,QAAO,IAAK;QAClE,IAAI,CAACtB,WAAU,GAAIsB,QAAQ,CAACvB,IAAI;MAClC,CAAC,CAAC,CAACwB,KAAK,CAACC,KAAI,IAAK;QAChBC,OAAO,CAACD,KAAK,CAAC,gDAAgD,EAAEA,KAAK,CAAC;MACxE,CAAC,CAAC;IACJ,CAAC;IACDP,cAAcA,CAAA,EAAG;MACfpB,yBAAyB