n          </div>\n          <div class=\"field\">\n            <label class=\"label\">Phone</label>\n            <div class=\"control\">\n              <input class=\"input\" type=\"text\" v-model=\"subscriber.phone\" required>\n            </div>\n          </div>\n          <div class=\"field\">\n            <label class=\"label\">Email</label>\n            <div class=\"control\">\n              <input class=\"input\" type=\"email\" v-model=\"subscriber.email\" required>\n            </div>\n          </div>\n          <div class=\"field\">\n            <label class=\"label\">Category</label>\n            <div class=\"control\">\n              <div class=\"select\">\n                <select v-model=\"subscriber.category\" required>\n                  <option v-for=\"category in categories\" :key=\"category._id\" :value=\"category._id\">{{ category.name }}</option>\n                </select>\n              </div>\n            </div>\n          </div>\n          <div class=\"field\">\n            <label class=\"label\">Type</label>\n            <div class=\"control\">\n              <div class=\"select\">\n                <select v-model=\"subscriber.stype\" required>\n                  <option v-for=\"type in types\" :key=\"type._id\" :value=\"type._id\">{{ type.name }}</option>\n                </select>\n              </div>\n            </div>\n          </div>\n        </form>\n      </section>\n      <footer class=\"modal-card-foot\">\n        <button class=\"button is-success\" @click=\"save\">{{ subscriber._id ? 'Save changes' : 'Add Subscriber' }}</button>\n        <button class=\"button\" @click=\"$emit('close')\">Cancel</button>\n      </footer>\n    </div>\n  </div>\n</template>\n\n<script>\nexport default {\n  props: {\n    show: Boolean,\n    subscriber: Object,\n    categories: Array,\n    types: Array\n  },\n  methods: {\n    save() {\n      this.$emit('save', this.subscriber);\n    }\n  }\n};\n</script>\n\n<style scoped>\n.modal-card {\n  max-width: 640px;\n}\n</style>\n"],"mappings":"AA6EA,eAAe;EACbA,KAAK,EAAE;IACLC,IAAI,EAAEC,OAAO;IACbC,UAAU,EAAEC,MAAM;IAClBC,UAAU,EAAEC,KAAK;IACjBC,KAAK,EAAED;EACT,CAAC;EACDE,OAAO,EAAE;IACPC,IAAIA,CAAA,EAAG;MACL,IAAI,CAACC,KAAK,CAAC,MAAM,EAAE,IAAI,CAACP,UAAU,CAAC;IACrC;EACF;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}